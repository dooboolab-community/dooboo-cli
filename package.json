{
  "name": "dooboo-cli",
  "version": "7.0.0",
  "description": "React and react-native starter kit",
  "bin": {
    "dooboo": "lib/bin/root.js"
  },
  "scripts": {
    "tsc": "tsc",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "build": "rm -rf lib/ && npm run lint && npm run tsc && cp -r templates lib/",
    "deploy": "npm run build && npm publish"
  },
  "author": "dooboolab",
  "license": "MIT",
  "keywords": [
    "react",
    "react-native",
    "expo",
    "cli",
    "template",
    "boilerplate",
    "typescript"
  ],
  "dependencies": {
    "boxen": "^6.2.1",
    "chalk": "^4.1.2",
    "commander": "^8.3.0",
    "inquirer": "^8.2.0",
    "ora": "6.0.1",
    "select-shell": "^1.1.3",
    "shelljs": "^0.8.4",
    "update-notifier": "^5.1.0"
  },
  "devDependencies": {
    "@dooboo/eslint-config": "^1.1.1",
    "@types/inquirer": "^8.1.3",
    "@types/node": "^16.11.4",
    "@types/ora": "^3.2.0",
    "@types/shelljs": "^0.8.9",
    "eslint": "^7.32.0",
    "typescript": "^4.4.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dooboolab/dooboo-cli.git"
  },
  "publishConfig": {
    "access": "public"
  }
}