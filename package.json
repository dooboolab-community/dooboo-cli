{
  "name": "dooboo-cli",
  "version": "8.7.1",
  "description": "React and react-native starter kit",
  "bin": {
    "dooboo": "lib/bin/root.js"
  },
  "scripts": {
    "tsc": "tsc",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "link": "npm run build && npm unlink -g && npm link",
    "build": "rm -rf lib/ && npm run lint && npm run tsc && cp -r templates lib/",
    "deploy": "npm run build && npm publish"
  },
  "author": "dooboolab",
  "license": "MIT",
  "keywords": [
    "react",
    "react-native",
    "expo",
    "cli",
    "template",
    "boilerplate",
    "typescript"
  ],
  "type": "module",
  "dependencies": {
    "boxen": "^7.0.0",
    "chalk": "^5.1.2",
    "commander": "^9.4.1",
    "inquirer": "^9.1.4",
    "ora": "6.1.2",
    "select-shell": "^1.1.3",
    "shelljs": "^0.8.5",
    "update-notifier": "^6.0.2"
  },
  "devDependencies": {
    "@dooboo/eslint-config-node": "^1.0.4",
    "@types/inquirer": "^9.0.3",
    "@types/keyv": "^3.1.4",
    "@types/node": "^18.11.9",
    "@types/ora": "^3.2.0",
    "@types/shelljs": "^0.8.11",
    "@types/update-notifier": "^6.0.1",
    "eslint": "^8.28.0",
    "jest": "^29.3.1",
    "prettier": "^2.8.0",
    "typescript": "^4.9.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dooboolab-community/dooboo-cli.git"
  },
  "publishConfig": {
    "access": "public"
  }
}
