{
  "name": "dooboo",
  "version": "0.1.4",
  "description": "Expo and expo router starter kit maintained by dooboolab.",
  "bin": {
    "dooboo": "lib/bin/root.js"
  },
  "scripts": {
    "tsc": "tsc",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "link": "npm run build && npm unlink -g && npm link",
    "build": "rm -rf lib/ && npm run lint && npm run tsc && cp -r templates lib/",
    "deploy": "npm run build && npm publish"
  },
  "author": "dooboolab",
  "license": "MIT",
  "keywords": [
    "react",
    "react-native",
    "expo",
    "cli",
    "template",
    "boilerplate",
    "typescript"
  ],
  "type": "module",
  "dependencies": {
    "boxen": "^7.1.0",
    "chalk": "^5.2.0",
    "commander": "^11.0.0",
    "inquirer": "^9.2.7",
    "ora": "6.3.1",
    "select-shell": "^1.1.3",
    "shelljs": "^0.8.5",
    "update-notifier": "^6.0.2"
  },
  "devDependencies": {
    "@dooboo/eslint-config-node": "^1.2.2",
    "@types/inquirer": "^9.0.3",
    "@types/keyv": "^3.1.4",
    "@types/node": "^20.3.1",
    "@types/ora": "^3.2.0",
    "@types/shelljs": "^0.8.12",
    "@types/update-notifier": "^6.0.4",
    "eslint": "^8.43.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "typescript": "5.0.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dooboolab-community/dooboo-cli.git"
  },
  "publishConfig": {
    "access": "public"
  }
}
